local Decimals = 4
local Clock = os.clock()

local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Mixvpnv2/Meow/refs/heads/main/Meow"))({
    cheatname = "Cattestate Dev",
    gamename = "Operation One",
})

library:init()

local Window1  = library.NewWindow({
    title = "main | de",
    size = UDim2.new(0, 510, 0.6, 6
)})

local Tab1 = Window1:AddTab("  Tab1  ")
local SettingsTab = library:CreateSettingsTab(Window1)

local Section1 = Tab1:AddSection("Section 1", 1)

Section1:AddToggle({
    text = "Toggle",
    state = false,
    risky = true,
    flag = "Toggle_1",
    risky = true,
    callback = function(v)

end
})
Section1:AddToggle({
    text = "Toggle",
    state = false,
    risky = true,
    flag = "Toggle_1",
    risky = false,
    callback = function(v)

    end
}):AddBind({
    enabled = true,
    text = "Keybind1",
    mode = "toggle",
    bind = "None",
    flag = "ToggleKey_1",
    state = false,
    nomouse = false,
    risky = false,
    noindicator = false,
    callback = function(v)
        print(ValueText, v)
    end,
    keycallback = function(v)
        print(ValueText, v)
    end
})

Section1:AddBox({
    enabled = true,
    name = "TextBox1",
    flag = "TextBox_1",
    input = "PlaceHolder1",
    focused = false,
    risky = false,
    callback = function(v)
        print(ValueText, v)
    end
})

Section1:AddSeparator({
    enabled = true,
    text = "Separator1"
})

Section1:AddButton({
    enabled = true,
    text = "Button1",
    confirm = true,
    risky = false,
    callback = function()
        print("Pressed!")
    end
})

Section1:AddSlider({
    enabled = true,
    text = "Slider1",
    flag = "Slider_1",
    suffix = "",
    dragging = true,
    focused = false,
    min = 0,
    max = 100,
    increment = 0.1,
    risky = false,
    callback = function(v)
        print(ValueText, v)
    end
})

Section1:AddList({
    enabled = true,
    text = "Selection", 
    selected = "",
    multi = false,
    open = false,
    max = 4,
    values = {"1", "2", "3"},
    risky = false,
    callback = function(v)
        print(ValueText, v)
    end
})

Section1:AddColor({
    enabled = true,
    text = "Color1",
    color = Color3.fromRGB(255, 255, 255),
    flag = "Color_1",
    trans = 0,
    open = false,
    risky = false,
    callback = function(v)
        print(ValueText, v)
    end
})

local Time = (string.format("%."..tostring(Decimals).."f", os.clock() - Clock))
library:SendNotification(("Loaded In "..tostring(Time)), 6)
